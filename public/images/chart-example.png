<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>
  
  <!-- Title -->
  <text x="200" y="30" text-anchor="middle" font-family="Arial" font-size="18" font-weight="bold" fill="#333">
    แผนภูมิตัวอย่างการประเมินคุณภาพ ONESQA
  </text>
  
  <!-- Chart Area -->
  <rect x="50" y="50" width="300" height="200" fill="white" stroke="#dee2e6" stroke-width="1"/>
  
  <!-- Y-axis labels -->
  <text x="45" y="60" text-anchor="end" font-family="Arial" font-size="12" fill="#666">100</text>
  <text x="45" y="100" text-anchor="end" font-family="Arial" font-size="12" fill="#666">75</text>
  <text x="45" y="140" text-anchor="end" font-family="Arial" font-size="12" fill="#666">50</text>
  <text x="45" y="180" text-anchor="end" font-family="Arial" font-size="12" fill="#666">25</text>
  <text x="45" y="220" text-anchor="end" font-family="Arial" font-size="12" fill="#666">0</text>
  
  <!-- X-axis labels -->
  <text x="90" y="270" text-anchor="middle" font-family="Arial" font-size="11" fill="#666">การบริหาร</text>
  <text x="150" y="270" text-anchor="middle" font-family="Arial" font-size="11" fill="#666">หลักสูตร</text>
  <text x="210" y="270" text-anchor="middle" font-family="Arial" font-size="11" fill="#666">นักศึกษา</text>
  <text x="270" y="270" text-anchor="middle" font-family="Arial" font-size="11" fill="#666">อาจารย์</text>
  <text x="330" y="270" text-anchor="middle" font-family="Arial" font-size="11" fill="#666">วิจัย</text>
  
  <!-- Bars -->
  <rect x="75" y="90" width="30" height="130" fill="#3e8ef7"/>
  <rect x="135" y="70" width="30" height="150" fill="#28a745"/>
  <rect x="195" y="100" width="30" height="120" fill="#ffc107"/>
  <rect x="255" y="80" width="30" height="140" fill="#dc3545"/>
  <rect x="315" y="110" width="30" height="110" fill="#6f42c1"/>
  
  <!-- Values on bars -->
  <text x="90" y="85" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="white">85</text>
  <text x="150" y="65" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="white">92</text>
  <text x="210" y="95" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="white">78</text>
  <text x="270" y="75" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="white">88</text>
  <text x="330" y="105" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="white">72</text>
  
  <!-- Grid lines -->
  <line x1="50" y1="100" x2="350" y2="100" stroke="#e9ecef" stroke-width="1"/>
  <line x1="50" y1="140" x2="350" y2="140" stroke="#e9ecef" stroke-width="1"/>
  <line x1="50" y1="180" x2="350" y2="180" stroke="#e9ecef" stroke-width="1"/>
</svg>